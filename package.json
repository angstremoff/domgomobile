{
  "name": "domgomobile",
  "version": "1.0.4",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "android:emu": "bash ./scripts/run_android.sh",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "analyze-bundle": "node scripts/analyze-bundle-size.js",
    "clean-console": "node scripts/replace-console-logs.js",
    "optimize": "npm dedupe && npm prune && node scripts/analyze-bundle-size.js",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "npm run lint && npm run typecheck && npm test"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/checkbox": "^0.5.16",
    "@react-native-material/core": "^1.3.7",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@sentry/react-native": "~6.10.0",
    "@supabase/supabase-js": "^2.39.3",
    "expo": "~52.0.47",
    "expo-application": "~6.0.2",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.8",
    "expo-image": "~2.0.7",
    "expo-image-manipulator": "~13.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-location": "~18.0.10",
    "expo-modules-core": "~2.2.3",
    "expo-navigation-bar": "~3.0.2",
    "expo-status-bar": "~2.0.1",
    "i18next": "^23.7.16",
    "intl-pluralrules": "^2.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^14.0.0",
    "react-native": "0.76.9",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.39.1",
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/cli": "^18.0.0",
    "@types/react": "~18.3.24",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^9.14.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react-native-dotenv": "^3.4.11",
    "sharp": "^0.34.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.47.0",
    "vitest": "^1.6.0"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@react-native-community/checkbox",
          "react-native-fast-image",
          "@react-native-material/core",
          "intl-pluralrules",
          "react-native-osmdroid"
        ]
      }
    }
  }
}
