{
  "cli": {
    "version": ">= 0.52.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true
    },
    "production": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "production",
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    }
  },
  "github": {
    "autoSubmit": true,
    "branches": ["main"],
    "workflow": {
      "preview": {
        "platforms": ["android"],
        "track": "preview"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
